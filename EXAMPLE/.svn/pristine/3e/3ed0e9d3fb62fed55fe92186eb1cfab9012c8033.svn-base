    ];
};

/**
 * 버튼 [GridView에 Data 할당하기] 클릭 시
 */
scwin.btn_ex1_onclick = function(e) {
    dlt_books_1.setJSON(scwin.getData());
};

/**
 * 버튼 [GridView에 Data 할당하기] 클릭 시
 */
scwin.btn_ex2_onclick = function(e) {
    dlt_books_2.setJSON(scwin.getData());
};
