    				<xf:group class="example_div_td_style" id="" style="">
    					<w2:textbox class="ws5_example_txt_normal" escape="false" id=""
    						label="GridView에 지정된 높이는 70px입니다. 높이를 가늠하기 위해 배경색이 지정되어 있습니다." style="">
    					</w2:textbox>
    					<w2:textbox class="ws5_example_txt_normal" escape="false" id=""
