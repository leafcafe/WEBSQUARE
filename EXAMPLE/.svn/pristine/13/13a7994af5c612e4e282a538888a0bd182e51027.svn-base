  counter-reset: ordered 0;
}

div, p, dl, dt, dd, ol, ul, li {
  margin: 0;
  padding: 0;
}

#cover_image img {
  max-width: 100%;
}

#cover_title h1 {
  border-bottom: 1px solid rgb(237, 237, 237);
}

.element {
  word-wrap: break-word;
}

.omit {
  display: none;
}

.image img {
  max-width: 100%;
}

.to_left {
  text-align: left !important;
}

.to_right {
  text-align: right !important;
}

.to_center {
  text-align: center !important;
}

table .to_bottom {
  vertical-align: bottom !important;
}

table .to_middle {
  vertical-align: middle !important;
}

#brief_toc ul li {
  list-style-type: none;
  margin: 15px 0;
  margin-left: 0;
}

#content h1.chapter_title {
  margin-bottom: 1.5em;
}

span.part_title_n, span.chapter_title_n, span.appendix_title_n {
  margin-right: 1em;
}

#content ol.ordered_list > li {
  counter-increment: ordered;
  list-style: none !important;
}

#content ol.ordered_list > li > p:before {
  content: counter(ordered) ". ";
  display: inline-block;
  margin-left: -20px;
  padding-right: 10px;
}

#content ol.ordered_list.renumbered {
  counter-reset: ordered 0;
}

sup.footnote {
  margin: 0 5px;
  font-weight: bold;
}

.footnotes a.to_content {
  margin-left: 10px;
  text-decoration: none !important;
}

#content table {
  border-collapse: collapse;
  table-layout: fixed;
  width: 100%;
}

#content th, #content td {
  vertical-align: top;
}

#content pre {
  white-space: pre-wrap;
  word-break: break-all;
}

#content kbd {
  background-color: rgb(247, 247, 247);
  border: 1px solid rgb(204, 204, 204);
  border-radius: 3px;
  box-shadow: 0 1px 0px rgba(0, 0, 0, 0.2),0 0 0 2px rgb(255, 255, 255) inset;
  color: rgb(51, 51, 51);
  font-size: 12px;
  font-weight: normal;
  margin: 0 0.1em;
  padding: 0.1em 0.6em;
  text-shadow: 0 1px 0 rgb(255, 255, 255);
  vertical-align: 2px;
  white-space: nowrap;
}

#content span.mark {
  display: none;
}

#content #index_group li {
  display: inline;
  font-size: 1.3em;
  font-weight: bold;
  margin: 0 15px 0 0;
}

#content #index_entries {
  padding-top: 30px;
}

#content #index_entries .index_group {
  clear: both;
  padding-top: 50px;
}

#content #index_entries h1 {
  border-top: 1px solid rgb(207, 207, 207);
  border-bottom: none;
  color: rgb(0, 102, 204);
  font-size: 1.5em;
  font-weight: bold;
  margin: 0 0 20px 0;
  padding-top: 5px;
}

#content #index_entries ul {
  float: left;
  margin: 0 20px 5px 0;
  padding: 0;
  width: 200px;
}

#content #index_entries ul ul {
  float: none;
  margin: 0;
  width: 100%;
}

#content #index_entries li {
  line-height: 1.5;
  list-style: none;
  margin: 0;
  padding-left: 2em;
  text-indent: -2em;
}

#content #index_entries li li {
  padding-left: 1em;
}

#content #index_entries ul li:first-letter {
  text-decoration: underline;
}

#content #index_entries ul ul li:first-letter {
  text-decoration: none;
}

#content #index_entries ul span a {
  margin-left: 5px;
  padding: 0 5px;
  text-decoration: none;
}

#content #index_entries ul span a:hover {
  background: rgb(244, 246, 248);
  outline: 1px solid rgb(207, 207, 207);
}

.code {
  counter-reset: code_line_number;
}

.code.support_code_numbering {
  overflow-x: auto;
  white-space: pre !important;
  word-wrap: normal !important; 
}

.code .code_line:after {
  color: rgb(160, 160, 160);
  content: counter(code_line_number);
  counter-increment: code_line_number;
  display: inline-block;
  padding-right: 0.9em;
  text-align: right;
  width: 1em;
}

.code .digits_10:after {
   width: 1.6em;
}

.code .digits_100:after {
   width: 2.2em;
}

.code .digits_100:after {
   width: 2.8em;
}

.code .keyword {
  color: rgb(127, 0, 85);
  font-weight: bold;
}

.code .string,
.code .css_value,
.code .attribute_value {
  color: rgb(42, 0, 255);
}

.code .attribute_name {
  color: rgb(127, 0, 127);
}

.code .attribute_value {
  font-style: italic;
}

.code .comment {
  color: rgb(63, 127, 95);
}

.code .selector {
  color: rgb(127, 0, 85);
  font-style: italic;
}

.code .tag_selector {
  color: rgb(127, 0, 85);
  font-weight: bold;
}

.code .dynamic_script {
  color: rgb(191, 95, 63);
}