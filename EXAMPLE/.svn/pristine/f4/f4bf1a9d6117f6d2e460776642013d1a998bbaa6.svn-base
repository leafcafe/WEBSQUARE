    				<xf:group class="example_div_th_style" id="">
    					<w2:span class="mr_def txt_blue" id="" label="로그 확인" style=""></w2:span>
    					<xf:trigger class="com_example_btn_log_clear" ev:onclick="scwin.btn_clearLog_onclick" id="btn_clearLog" style=""
