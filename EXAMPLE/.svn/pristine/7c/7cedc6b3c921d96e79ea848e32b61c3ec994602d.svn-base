												style="height:20px;" value="파일명" width="199">
											</w2:column>
											<w2:column blockSelect="false" displayMode="label" id="column1" inputType="text"
												style="height:20px;" value="사이즈(byte)" width="88">
											</w2:column>
										</w2:row>
									</w2:header>
									<w2:gBody id="gBody1" style="">
