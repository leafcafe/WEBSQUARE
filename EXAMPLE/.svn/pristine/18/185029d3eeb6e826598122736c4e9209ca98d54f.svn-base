scwin.btn_setGridHeight_onclick = function(e) {
	var tmpHeight = ibx_gridHeight.getValue();
	
	if(!tmpHeight){
		alert("변경할 그리드의 높이를 입력하세요.");
		ibx_gridHeight.focus();
		return;
