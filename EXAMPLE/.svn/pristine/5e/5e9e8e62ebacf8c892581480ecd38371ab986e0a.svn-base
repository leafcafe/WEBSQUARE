    				<xf:group class="example_div_td_style" id="" style="">
    					<w2:spinner buttonSize="30" id="spi_exam2" inputReadOnly="false" skin="type1"
    						style="position: relative;width:130px;min-height:24px;" dataType="date" useAlert="false">
    					</w2:spinner>
    				</xf:group>
    			</xf:group>

    		</xf:group>
