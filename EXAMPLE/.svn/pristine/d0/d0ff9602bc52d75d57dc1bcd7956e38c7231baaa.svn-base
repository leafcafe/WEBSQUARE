/* switch toggle checkbox  */
.input_switch{display:inline-block;height:22px;vertical-align:middle;margin-right:4px;}
.input_switch > .w2checkbox .w2checkbox_item{position:relative;height:22px;min-height:unset;line-height:unset;padding-left:0;}
.input_switch .w2checkbox_item input[type="checkbox"]{position:absolute;top:3px;left:4px;visibility:hidden;opacity:0;font-size:0;line-height:0;}
.input_switch .w2checkbox_label{overflow:hidden;position:relative;display:inline-block;width:57px;height:22px;background-color:#6bbbc5;color:#fff;font-size:12px;cursor: pointer; border-radius: 13px; transition: all .2s;}
.input_switch .w2checkbox_label:before{content:'작게';position: absolute;top:11px;left:3px;display:block;width:16px;height:16px;line-height:16px;color:#fff;text-indent:20px;border:0;background-color:#fff;white-space:nowrap;border-radius: 50%; transition: all .2s;}
.input_switch .w2checkbox_label:after{display:none;line-height:20px;}
.input_switch input[type="checkbox"]:checked + .w2checkbox_label{background-color:#29679e;}
.input_switch input[type="checkbox"]:checked + .w2checkbox_label:before{left:37px;background-color:#fff;}
.input_switch input[type="checkbox"]:checked + .w2checkbox_label:after{content:'크게';display:block;position:absolute;top:5px;left:9px;width:auto;height:auto;color:#fff;border:0;transform:none;}

