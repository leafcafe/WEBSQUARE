/* pageTab */
.tabc_layout{position:absolute;left:0;right:0;top:0;bottom:0}
.tabc_layout > .w2tabcontrol_control{position:absolute;height:30px;}
.tabc_layout > .w2tabcontrol_control > .w2tabcontrol_control:before{content:'';position:absolute;left: 0px;top:0px;z-index:1;width:100%;}
