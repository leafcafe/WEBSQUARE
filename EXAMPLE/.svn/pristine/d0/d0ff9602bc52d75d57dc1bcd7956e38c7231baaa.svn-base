.win_c .w2windowContainer_selectedNameLayer .w2windowContainer_nameLayer_close {background:url(../images/contents/tab_close_active.png) no-repeat center center; width:25px; height:25px; right:0px; left:auto; top:0px;}
.win_c .w2windowContainer_controlIconLayer{width:209px !important;right:0;top:0;height:30px;background: transparent;}
