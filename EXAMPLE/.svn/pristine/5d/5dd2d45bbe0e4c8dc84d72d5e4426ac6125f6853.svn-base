					</xf:trigger>
					<w2:span label="|" style="line-height: 26px;font-size: 10px;margin: 0px 4px;color: #fff;opacity: 0.5;" id="spa_gen_bar"></w2:span>
				</w2:generator>
			</xf:group>
			<xf:group id="grp_container" class="container">
