<hashtable>
	<data hashkey="component">
		<properties>
			<property default="" description="component명"
				name="meta_name" necessary="no" type="string" />
			<property default="" description="component 설명"
				name="meta_desc" necessary="no" type="multiline" />
		</properties>
	</data>
	<data hashkey="dataCollection">
		<properties>
			<property default="" description="dataCollection 명"
				name="meta_name" necessary="no" type="string" />
			<property default="" description="dataCollection 설명"
				name="meta_desc" necessary="no" type="multiline" />
		</properties>
	</data>
	<data hashkey="head">
		<properties>
			<property default="" description="화면 ID"
				name="meta_screenId" necessary="no" type="string" />
			<property default="" description="화면명"
				name="meta_screenName" necessary="no" type="string" />
			<property default="" description="단위업무명"
				name="meta_subsystem" necessary="no" type="string" />
			<property default="" description="작성자" name="meta_author"
				necessary="no" type="string" />
			<property default="" description="화면 유형" name="meta_type"
				necessary="no" type="combobox">
				<option name="메인" value="메인" />
				<option name="서브" value="서브" />
				<option name="팝업" value="팝업" />
			</property>
			<property default="" description="개요" name="meta_desc"
				necessary="no" type="multiline" />
			<property default="" description="기능 정의" name="meta_process"
				necessary="no" type="multiline" />
			<property default="" description="비고" name="meta_etc"
				necessary="no" type="multiline" />
		</properties>
	</data>
	<data hashkey="event">
		<properties>
			<property default="" description="event명" name="metaev_name"
				necessary="no" type="string" />
			<property default="" description="event 설명"
				name="metaev_desc" necessary="no" type="multiline" />
			<property default="" description="event 처리절차"
				name="metaev_process" necessary="no" type="multiline" />
			<property default="" description="event 업무규칙"
				name="metaev_rule" necessary="no" type="multiline" />
			<property default="" description="비고" name="metaev_etc"
				necessary="no" type="multiline" />
		</properties>
	</data>
	<data hashkey="history">
		<properties>
			<property default="" description="개정번호" hidden="N"
				name="meta_no" necessary="Y" type="string" />
			<property default="" description="제/개정 페이지 및 수정 내용"
				hidden="N" name="meta_desc" necessary="Y" type="multiline" />
			<property default="" description="제/개정 일자" hidden="N"
				name="meta_date" necessary="Y" type="string" />
			<property default="" description="제/개정자" hidden="N"
				name="meta_user" necessary="Y" type="string" />
		</properties>
	</data>
	<data hashkey="metaConfig">
		<metainfo engmeta="" kormeta="" lengthmeta="" typemeta=""
			use="false" virtualpointmeta="">
			<rtrvmeta encoding="UTF-8" mediatype="application/json"
				timeout="30" url="http://" />
			<changemeta encoding="UTF-8" mediatype="application/json"
				timeout="30" url="http://" />
		</metainfo>
	</data>
	<data hashkey="workflow">
		<properties>
			<property default="" description="submission 명"
				name="meta_name" necessary="no" type="string" />
			<property default="" description="submission 설명"
				name="meta_desc" necessary="no" type="multiline" />
			<property default="" description="submission 처리절차"
				name="meta_process" necessary="no" type="multiline" />
			<property default="" description="submission 업무규칙"
				name="meta_rule" necessary="no" type="multiline" />
			<property default="" description="비고" name="meta_etc"
				necessary="no" type="multiline" />
		</properties>
	</data>
	<data hashkey="submission">
		<properties>
			<property default="" description="submission 명"
				name="meta_name" necessary="no" type="string" />
			<property default="" description="submission 설명"
				name="meta_desc" necessary="no" type="multiline" />
			<property default="" description="submission 처리절차"
				name="meta_process" necessary="no" type="multiline" />
			<property default="" description="submission 업무규칙"
				name="meta_rule" necessary="no" type="multiline" />
			<property default="" description="비고" name="meta_etc"
				necessary="no" type="multiline" />
		</properties>
	</data>
</hashtable>
