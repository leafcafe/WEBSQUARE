};

// 그리드뷰 1번 footer 총 건수 표시 형식
scwin.dis = function (data) {
    return "총 " + data + " 건";
};

// 지움 버튼 - 페이지 다시 로드
scwin.btn_reset_onclick = function (e) {
    $p.reinitialize();
};


// 다운로드 버튼
scwin.btn_down_onclick = function (e) {
    // 옵션 변수
    var options = {};
    // xlsx, xls, csv 구분 ex) .csv

    // 옵션 설정
    options.fileName = "excel.xlsx";
    options.type = select_type1.getValue();

    // 컬럼 무브를 적용해 다운 받을지 여부
    options.columnMove = "true";
    // 스타일 사용 여부
    options.useStyle = "true";
    // footer 포함 여부
    options.useFooter = "true";
    // 다운로드
    grd1.advancedExcelDownload(options);
};

// 업로드 버튼 클릭 이벤트
scwin.btn_up_onclick = function (e) {
    var options = {};
    var type = select_type1.getValue();

    options.type = type;
    // 업로드 창 modal 사용 여부
    options.useModalDisable = "true";
    // 헤더 존재 여부
    options.headerExist = "1";

    options.processMsg = "처리중입니다.";

    // csv 타입인 경우
    if (type == ".csv") {
        options.delim = ";"
        grd1.readCSV(options);
        return;
    }
    // 업로드
    grd1.advancedExcelUpload(options);

};

// clear 버튼 클릭 이벤트 - 그리드뷰1번 초기화
scwin.btn_clean_onclick = function (e) {
    dl_gridview1.setJSON([]);
};

scwin.masking_formatter = function (data) {
    var len = data.length;
    var str = "";
    if (len <= 0) {
        return "";
    }
    for (var i = 0; i < len - 2; i++)
        str += '*';
    return str + "증권";
};
scwin.btn_search_onclick = function (e) {
    $p.executeSubmission("sbm_gridview1");
};

scwin.btn_insertRow1_onclick = function (e) {
    dl_gridview1.insertRow(0);
}

// 2줄 그리드 기능 처리
// 다운로드
scwin.btn_down2_onclick = function (e) {
    // 옵션 변수
    var options = {};
    // xlsx, xls, csv 구분 ex) .csv

    // 옵션 설정
    options.fileName = "excel.xlsx";
    options.type = select_type2.getValue();

    // 컬럼 무브를 적용해 다운 받을지 여부
    options.columnMove = "true";
    // 스타일 사용 여부
    options.useStyle = "true";
    // footer 포함 여부
    options.useFooter = "true";
    options.showProcess = true;
    // 다운로드
    grd2.advancedExcelDownload(options);
};

//업로드
scwin.btn_up2_onclick = function (e) {
    var options = {};
    var type = select_type2.getValue();
    options.type = type;
    // 업로드 창 modal 사용 여부
    options.useModalDisable = "true";
    // 헤더 존재 여부
    options.headerExist = "1";
    options.processMsg = "업로드중입니다.";

    // csv 타입인 경우
    if (type == ".csv") {
        options.delim = ";"
        grd1.readCSV(options);
        return;
    }
    // 업로드
    grd2.advancedExcelUpload(options);
};

//초기화
scwin.btn_clean2_onclick = function (e) {
    dl_gridview2.setJSON([]);
};

scwin.btn_insertRow2_onclick = function (e) {
    dl_gridview2.insertRow(0);
}

//조회
scwin.btn_search2_onclick = function (e) {
    $p.executeSubmission("sbm_gridview2");
};

// 단축키
scwin.complete = function (e) {
    if (e.key == "F1") {
        var fr = $p.getParentWindow();
        fr.scwin.showHelp("p19");
    } else if (e.key == "F2") {
        scwin.btn_search_onclick(e);
        scwin.btn_search2_onclick(e);
    }
}
scwin.grd_oncustompaste = function (data) {

    var dataArr = data.split("\r\n");
    var retArr = [];
    for (var i = 0; i < dataArr.length; i++) {
        var rowArr = dataArr[i].split("\t");
        for (var j = 0; j < rowArr.length; j++) {
            rowArr[j] = rowArr[j].replaceAll(",", "");
        }
        retArr.push(rowArr);
    }

    var jsonToCsv = Papa.unparse(retArr, {
        delimiter: "\t"
    });
    return jsonToCsv;
}
]]></script>
