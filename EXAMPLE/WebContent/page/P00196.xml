// 페이지 로드 시 실행
scwin.onpageload = function () {
    //$p.executeSubmission("sbm_gridview1");
};

// 검색 버튼 클릭 이벤트 - 그리드뷰 1번 데이터 submission 실행
scwin.btn_search_onclick = function (e) {
